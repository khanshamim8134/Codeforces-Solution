#include<bits/stdc++.h>
using namespace std;

typedef long long int ll;
typedef unsigned long long int llu;
typedef double dl;

#define endl '\n'
#define PI acos(-1)
#define vi vector<int>
#define vll vector<ll>
#define vii vector<pair<int,int>>


int main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie();
    ll t,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        if(n==1||n==3)
        {
            cout<<-1<<endl;
        }
        else if(n==2)
        {
            cout<<66<<endl;
        }
        else if(n==5)
        {
            cout<<36366<<endl;
        }
        else if(n%2==1)
        {
            for(int i=1; i<=(n-4); i++){
                cout<<"3";
            }
            cout<<"6366"<<endl;
        }
        else
        {
            for(int i=1; i<=n-2; i++){
                cout<<"3";
            }
            cout<<"66"<<endl;
        }
    }

    return 0;
}

